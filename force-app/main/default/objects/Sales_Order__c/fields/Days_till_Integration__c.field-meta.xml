<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Days_till_Integration__c</fullName>
    <description>Indicate how many till this sales order is pushed to Pronto</description>
    <externalId>false</externalId>
    <formula>If(Order_Date__c - Today() + 2 &lt; 0, 0,Order_Date__c - Today() + 2)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Days till integration</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
